<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly ="LojaLimpeza"
                   namespace="LojaLimpeza">
  <class name="Produto">
    <id name="Codigo">
      <generator clas="identity"/>
    </id>
    <property name="Nome"/>
    <property name="Valor"/>
    <property name="Categoria"/>
    <property name="DataFabricacao"/>
    <property name="DataValidade"/>
    <property name="Quantidade"/>

  </class>
</hibernate-mapping>