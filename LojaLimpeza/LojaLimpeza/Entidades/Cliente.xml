<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="LojaLimpeza"
                   namespace="LojaLimpeza">
  <class name="Cliente">
    <id name="IdCliente">
      <generator class="identity"/>
    </id>
    <property name="NomeCliente"/>
    <property name="CPF"/>
    <property name="Filiacao"/>
    <property name="Contato"/>
    <property name="Endereco"/>
    <property name="StatusDoCliente"/>
    <property name="LimiteCredito"/>
    <property name="ListaDePedido"/>
  </class>
  
</hibernate-mapping>